
# Utilities
alias cl='clear'
alias v='nvim'
alias vim='nvim'
alias ls='ls --color=auto'
alias ll='ls -l'
alias la='ls -la'
alias denowatch='deno run --allow-all --watch'
# alias dev='fd --type d --hidden --exclude .git --max-depth 1 | fzf-tmux -p | xargs nvim'
alias fdfzfdev='fd . "/Users/kevingyori/dev/" --type d --max-depth 1 | fzf-tmux -p'
alias cdev='cd "$(fdfzfdev)"'
alias dev='cdev && nvim .'

# tmux
alias tma='tmux a'
alias tm='tmux'
alias tmls='tmux ls'
alias tmc='tmux new -s'
alias tmd='tmux detach'

# Configs
# alias config-starship='vim ~/.config/starship.toml'
alias config='vim ~/.dotfiles/zsh/.zshrc'
alias configa='vim ~/.dotfiles/zsh/.zsh_aliases'
alias configala='vim ~/.dotfiles/alacritty/.config/alacritty/alacritty.yml'
alias configkitty='vim ~/.dotfiles/kitty/.config/kitty/kitty.conf'
alias configtmux='vim ~/.dotfiles/tmux/.config/tmux/tmux.conf'
alias configyabai='vim ~/.dotfiles/yabai/.config/yabai/yabairc'
alias configskhd='vim ~/.dotfiles/skhd/.config/skhd/skhdrc'
alias configchad='vim ~/.config/nvim/lua/custom/'

alias reload='source ~/.zshrc'
alias reloadtmux='tmux source-file ~/.config/tmux/tmux.conf'

alias bench='~/.zsh-bench/zsh-bench'
